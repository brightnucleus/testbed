{
	"name": "brightnucleus/testbed",
	"description": "Testing grounds for the different Bright Nucleus components.",
	"type": "wordpress-plugin",
	"homepage": "https://www.brightnucleus.com/",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Alain Schlesser",
			"homepage": "https://www.brightnucleus.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://www.github.com/brightnucleus/testbed/issues"
	},
	"autoload": {
		"psr-4": {
			"BrightNucleus\\Testbed\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"composer/installers": "^1.0",
		"brightnucleus/config": ">=0.2",
		"brightnucleus/dependencies": ">=0.2",
		"brightnucleus/shortcodes": ">=0.2",
		"brightnucleus/custom-content": "^0.1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8"
	}
}
